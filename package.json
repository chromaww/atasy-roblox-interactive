{
  "name": "atasy-roblox-interactive",
  "productName": "atasy-roblox-interactive",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": {
    "name": "chromaww",
    "email": "bachtiar.cahyoo@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@nut-tree-fork/nut-js": "^4.2.6",
    "electron-squirrel-startup": "^1.0.1",
    "tiktok-live-connector": "^2.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron/fuses": "^1.8.0",
    "electron": "37.3.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.atasy.robloxinteractive",
    "productName": "Atasy Roblox Interactive",
    "files": [
      "main.js",
      "preload.js",
      "src/**/*",
      "node_modules/**/*",
      "assets/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.png",
      "artifactName": "TikTokRobloxBot-${version}-Setup.exe"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
